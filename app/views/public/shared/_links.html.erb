<%- if controller_name != 'sessions' %>
  <h4><%= link_to "ユーザーログインページへ移動する", new_session_path(resource_name), class: 'btn btn-green mt-5' %><br /></h4>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <h4><%= link_to 'ユーザー新規登録ページへ移動する', new_registration_path(resource_name), class: 'btn btn-green mt-5' %><br /></h4>
<% end %>
